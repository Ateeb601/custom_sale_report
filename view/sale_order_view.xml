<odoo>
  <record id="view_sale_order_form_inherit" model="ir.ui.view">
    <field name="name">sale.order.form.inherit</field>
    <field name="model">sale.order</field>
    <field name="inherit_id" ref="sale.view_order_form"/>  <!--    form view ki exteral ID is ref =""-->
    <field name="arch" type="xml">
      <header position="inside">
        <button name="action_open_sale_report_wizard" type="object" string="Custom Sale Report" class="oe_highlight"/>
      </header>

    <xpath expr="//group/field[@name='pricelist_id']" position="before">
            <field name="appointment_id"/>
        </xpath>
 </field>
  </record>
</odoo>
