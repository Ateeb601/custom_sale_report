<odoo>
  <record id="view_sale_order_report_wizard" model="ir.ui.view">
    <field name="name">sale.order.report.wizard</field>
    <field name="model">sale.order.report.wizard</field>
    <field name="arch" type="xml">
      <form string="Sale Order Report Wizard">
        <group>
          <field name="date_from"/>
          <field name="date_to"/>
        </group>
        <footer>
          <button name="action_print_report" type="object" string="Print" class="btn-primary"/>
          <button string="Cancel" class="btn-secondary" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
